Cliente por ID:
query {
  cliente(id: 1) {
    ... on Cliente {
      id
      nome
      email
    }
    ... on objErr {
      err {
        path
        message
      }
    }
  }
}

Clientes cadastrados:
query {
  clientes {
    ... on Cliente {
      result {
        id
        nome
        email
        dataNasc
        cpf
        rua
        bairro
        cidade
        numero
        cep
      }
    }
    ... on objErr {
      err {
        path
        message
      }
    }
  }
}

Criar Cliente:
mutation {
  createCliente(cliente: {
    nome: "larissa"
    email: "larissapaganini@gamail.com"
    cpf: "15442311723"
    dataNasc: "27/10/2000"
    rua: "expe"
    bairro: "bairro alto"
    cidade: "iconha"
    estado: "es"
    pais: "brasil"
    cep: 29280000
    numero: "n/a"
  }) {
    ... on Cliente {
      	id
        nome
        email
        dataNasc
        cpf
        endereco {
          rua
        }
    }
    ... on objErr {
      err {
        path
        message
      }
    }
  }
}

Update cliente:
mutation {
  updateCliente(id: 2, dados: {
    nome: "Larissa Paganini"
    email: "larissapaganini@gamail.com"
    cpf: "15442311723"
    dataNasc: "27/10/2000"
    rua: "expe"
    bairro: "bairro alto"
    cidade: "iconha"
    estado: "es"
    pais: "brasil"
    cep: 29280000
    numero: "n/a"
  }) {
    ... on Cliente {
      	id
        nome
        email
        dataNasc
        cpf
        endereco {
          rua
        }
    }
    ... on objErr {
      err {
        path
        message
      }
    }
  }
}

Deleta Cliente:
mutation {
  deleteCliente(id: 3)
}

Produtos Cadastrados:
query {
  produtos {
    ... on Produtos {
      result {
        id
        nome
        preco
        peso
        estoque
      }
    }
    ... on objErr {
      err {
        path
        message
      }
    }
  }
}

Produto por ID:
query {
  produto(id: 2) {
    ... on Produto {
        id
        nome
        preco
        peso
        estoque
    }
    ... on objErr {
      err {
        path
        message
      }
    }
  }
}

Criar Produto:
mutation {
  createProduto(produto: {
    nome: "trem"
    imagem: "https://imagens.com/trem"
    descricao: "trem de 5 vagões"
    peso: 60.65
    preco: 20.99
    estoque: 200
  }) {
    ...on Produto {
      id
      nome
      descricao
      imagem
      peso
      preco
      estoque
    }
    ...on objErr {
      err {
        path
        message
      }
    }
  }
}

Update Produto:
mutation {
  updateProduto(id: 3, dados: {
    nome: "Trem"
    imagem: "https://imagens.com/trem"
    descricao: "trem de 5 vagões"
    peso: 60.65
    preco: 20.99
    estoque: 200
  }) {
    ...on Produto {
      id
      nome
      descricao
      imagem
      peso
      preco
      estoque
    }
    ...on objErr {
      err {
        path
        message
      }
    }
  }
}

Deleta Produto:
mutation {
  deleteProduto(id: 3)
}

Pedidos cadastrados:
query {
  pedidos {
    ...on Pedidos {
      result {
        id
        compradorId
        dataCriacao
        status
      }
    }
  }
}

Cria Pedido:
mutation {
  createPedido(pedido:{
    compradorId: 1
    parcelas: 5
    status: "aprovado"
  }, produtos: {
    produtos: [{quantidade: 1, id: 2}, {quantidade: 5, id: 1}]
  }) {
    ...on PedidoCriado {
      id
      total
      itens {
        quantidade
        nome
        preco
      }
      parcelas
      status
    }
    ...on objErr {
      err {
        path
        message
      }
    }
  }
}

Update Pedido:
mutation {
  updatePedido(id: 8, dados:{
    compradorId: 1
    parcelas: 5
    status: "enviado"
  }) {
    ...on Pedido {
      id
      compradorId
      status
    }
    ...on objErr {
      err {
        path
        message
      }
    }
  }
}

Pedido por id:
query {
  pedido(id: 2) {
    ...on Pedido {
      id
      parcelas
      compradorId
      status
      dataCriacao
    }
    ...on objErr {
      err {
        path
        message
      }
    }
  }
}

Resumo de pedido:
query {
  resumoPedido(id: 2) {
    ...on ResumoPedido {
      montante
      email
      pedido
      cliente
    }
    ...on objErr {
      err {
        path
        message
      }
    }
  }
}
